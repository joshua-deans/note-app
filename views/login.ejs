<div class="container" id="note-container">
	<div class="card hoverable z-depth-2" id="login-card">
		<div class="card-content">
			<span class="card-title">Login</span>
        	<form>
        		<input placeholder="Email" id="email" name="email" type="email" class="validate" required>
        		<input placeholder="Password" id="password" name="password" type="password" class="validate" required>
				<button class="btn waves-effect waves-light" name="submit" id="submit">Submit
				<i class="material-icons right">send</i>
				</button>
			</form>
		</div>
	</div>
</div>
